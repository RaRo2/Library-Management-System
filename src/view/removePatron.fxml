<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controller.RemovePatronController"
            alignment="center" styleClass="root" spacing="10"
>
    <ListView fx:id="patronLv" prefWidth="250" prefHeight="400">
    </ListView>
    <Button fx:id="removeBtn" text="Remove Patron" disable="true" onAction="#handleRemovePatron"/>
    <Button text="Close" onAction="#handleClose"/>
    <stylesheets>
        <URL value="@style.css"/>
    </stylesheets>
</VBox>